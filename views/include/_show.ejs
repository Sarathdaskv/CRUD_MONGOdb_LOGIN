<% for(let i=0;i<users.length;i++){ %>
    <tr>
       
        <td><%=users[i].name%></td>
        <td><%=users[i].email%></td>
        <td><%=users[i].position%></td>
        <td>
            <a href="/update-user?id=<%=users[i]._id%>" class="btn btn-outline-primary"><i
                    class="fa-solid fa-pencil "></i></a>
            <a  class="btn btn-outline-primary delete-btn" data-id="<%=users[i]._id%>"><i
                    class="fa-sharp fa-solid fa-trash"></i></a>
        </td>
    </tr>
    <% } %>
    